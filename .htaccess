<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteBase /mini-app/

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^ index.php [QSA,L]
</IfModule>

<FilesMatch "\.(env|json|lock|md|sql|yml|yaml)$">
    Require all denied
</FilesMatch>
